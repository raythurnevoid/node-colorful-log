{
	"name": "@raythurnevoid/node-colorful-log",
	"version": "0.2.0",
	"description": "",
	"scripts": {
		"clean-lib": "del-cli lib",
		"copy:package": "copyfiles package.json lib",
		"copy:src": "copyfiles --up 1 src/lib/*.* lib/src",
		"tsc": "tsc -p tsconfig.lib.json",
		"lib": "npm-run-all clean-lib copy:package copy:src tsc",
		"prepackage": "npm run lib",
		"package": "npm publish ./lib --access public",
		"env": "cross-env TS_NODE_PROJECT=\"tsconfig.json\"",
		"ts-node": "npm run env -- node -r ts-node/register -r tsconfig-paths/register --loader ts-node/esm --experimental-specifier-resolution=node --",
		"ts-node:inspect": "npm run ts-node -- --inspect --",
		"start": "npm run ts-node src/index.ts",
		"debug": "npm run ts-node:inspect src/index.ts",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"type": "module",
	"exports": {
		"./browser": "./lib/browser",
		"./node": "./lib/node"
	},
	"browserslist": [
		"> 5% or node >= 16"
	],
	"author": "Daniele Loreto <dloreto.work@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"@types/chalk": "^2.2.0",
		"chalk": "^4.1.1"
	},
	"devDependencies": {
		"@raythurnevoid/ts-node-template": "^0.0.5",
		"@types/jest": "^26.0.23",
		"jest": "^27.0.4",
		"ts-jest": "^27.0.3"
	}
}

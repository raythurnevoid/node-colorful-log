{
	"name": "@raythurnevoid/node-colorful-log",
	"version": "0.1.1",
	"description": "",
	"main": "./cjs/index.js",
	"module": "./esm/index.js",
	"scripts": {
		"env": "cross-env TS_NODE_PROJECT=\"tsconfig.json\"",
		"ts-node": "npm run env -- node -r ts-node/register -r tsconfig-paths/register --",
		"ts-node:inspect": "npm run ts-node -- --inspect --",
		"start": "npm run ts-node src/index.ts",
		"debug": "npm run ts-node:inspect src/index.ts",
		"clean-lib": "del-cli lib",
		"copy:package": "copyfiles package.json lib",
		"copy:src": "copyfiles --up 1 src/lib/*.* lib/src",
		"tsc:cjs": "tsc -p tsconfig.lib.cjs.json",
		"tsc:esm": "tsc -p tsconfig.lib.esm.json",
		"lib": "npm-run-all clean-lib copy:package copy:src tsc:cjs tsc:esm",
		"prepackage": "npm run lib",
		"package": "npm publish ./lib --access public",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/chalk": "^2.2.0"
	},
	"devDependencies": {
		"@raythurnevoid/ts-node-template": "^0.0.2"
	}
}
